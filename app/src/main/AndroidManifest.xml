<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoldesCMMS">
        
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.OperadoresActivity"
            android:parentActivityName=".ui.MainActivity" />
            
        <activity
            android:name=".ui.OperadorFormActivity"
            android:parentActivityName=".ui.OperadoresActivity" />

        <activity
            android:name=".ui.RegistroProduccionActivity"
            android:parentActivityName=".ui.MainActivity" />
            
        <activity
            android:name=".ui.RegistroProduccionFormActivity"
            android:parentActivityName=".ui.RegistroProduccionActivity" />

        <activity
            android:name=".ui.RequerimientosInsumoActivity"
            android:parentActivityName=".ui.MainActivity" />
            
        <activity
            android:name=".ui.RequerimientoInsumoFormActivity"
            android:parentActivityName=".ui.RequerimientosInsumoActivity" />
            
        <activity
            android:name=".ui.RequerimientoInsumoDetailActivity"
            android:parentActivityName=".ui.RequerimientosInsumoActivity" />

        <activity
            android:name=".ui.RefaccionesMaquinaActivity"
            android:parentActivityName=".ui.MainActivity" />
            
        <activity
            android:name=".ui.RefaccionMaquinaFormActivity"
            android:parentActivityName=".ui.RefaccionesMaquinaActivity" />
        <activity
            android:name=".ui.MoldesActivity"
            android:parentActivityName=".ui.MainActivity" />

        <activity
            android:name=".ui.MantenimientosActivity"
            android:parentActivityName=".ui.MainActivity" />

        <activity
            android:name=".ui.SolicitudesActivity"
            android:parentActivityName=".ui.MainActivity" />

        <activity
            android:name=".ui.MaquinasActivity"
            android:parentActivityName=".ui.MainActivity" />

        <activity
            android:name=".ui.CalidadActivity"
            android:parentActivityName=".ui.MainActivity" />

        <activity
            android:name=".ui.InventarioActivity"
            android:parentActivityName=".ui.MainActivity" />
    </application>

</manifest>
